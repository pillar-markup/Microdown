{ #category : #initialization }
MicHTMLCanvas class >> readNamedEntities [

	| htmlChars |
	htmlChars := Dictionary new.
	self namedEntities readStreamDo: [ :stream |
		(NeoCSVReader on: stream) 
			skip: 11;
			separator: Character tab;
			addField; 
			addField; 
			addField;
			do: [ : rowArray |
				rowArray third ifNotNil: [ : char |
					htmlChars 
						at: char first 
						put: '&' , rowArray first ] ] ].
	^ htmlChars.
]

{ #category : #initialization }
MicHTMLCanvas class >> reset [
	<script>

	HTMLCharacters := nil
]

{ #category : #accessing }
MicHTMLCanvas >> nextPut: aCharacter [

	(HTMLCharacters at: aCharacter ifAbsent: nil)
		ifNil: [ super nextPut: aCharacter ]
		ifNotNil: [ :string | self raw: string ]
]

{ #category : #accessing }
MicHTMLCanvas >> tag [
	^ self brush: MicHTMLTag new
]
